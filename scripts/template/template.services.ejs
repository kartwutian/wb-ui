import request from '<%=utilsPath%>/request'
<% if(list.length === 0){ %>
export function getDemo(params) {
    return request(`/api/ftts/user/login}`, {
    method: 'GET',
    data: params,
  }, false)
}

export function postTest(params) {
  return request(`/app/ajaxLogout`, {
    method: 'post',
    data: params,
  })
}
<% } %>

<% list.forEach(function(item){ %>
/**
*<%=item.desc%>
* @param params
**/
export function <%=item.name%>(params) {
  return request(`<%=item.path%>`, {
    method: '<%=item.method.toUpperCase()%>',
    data: params,
  })
}
<% }) %>
